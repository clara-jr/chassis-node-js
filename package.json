{
  "name": "chassis-node-js",
  "version": "1.0.0",
  "description": "Node.js + Express.js + MongoDB chassis",
  "license": "ISC",
  "author": "Clara JimÃ©nez Recio",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint . \"**/*.{js,json}\"",
    "lint:fix": "npx eslint . \"**/*.{js,json}\" --fix",
    "lint:quiet": "npx eslint . \"**/*.{js,json}\" --quiet",
    "lint:staged": "npx eslint . --fix",
    "pre-commit": "npx lint-staged",
    "pre-commit-msg": "npx --no -- commitlint --edit ${1}",
    "pre-push": "NODE_ENV=test npx c8 --all mocha",
    "prepare": "husky",
    "start": "node ./index.js",
    "start:dev": "NODE_ENV=dev node --watch ./index.js",
    "test": "NODE_ENV=test npx c8 --all mocha",
    "test:report": "NODE_ENV=test npx c8 --all --reporter=html -reporter=text mocha"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.1.1",
    "c8": "^9.1.0",
    "chai": "^5.1.0",
    "eslint": "^9.1.1",
    "eslint-plugin-json-format": "^2.0.1",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "mongodb-memory-server": "^9.2.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=22.1.0",
    "npm": ">=10.7.0"
  }
}